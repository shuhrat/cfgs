[user]
    name = Shuhrat Shadmonkulov
    email = zumra6a@yandex-team.ru
[core]
    mergeoptions = --no-ff
    pager = less -r
	excludesfile = /Users/zumra6a/.gitignore_global
[alias]
    vlog = log --pretty='%h (%an) %s' --graph
    st = status
    co = checkout
    cp = cherry-pick
    ci = commit
    up = pull
    di = diff
    br = branch
    lol = log --graph --decorate --pretty=oneline --abbrev-commit
    hist = log --pretty=format:'%C(yellow)%h%C(reset) %ad | %C(75)%s%C(reset) %C(yellow)%d%C(reset) [%an]' --graph --date=short --branches --all
    hash = show -s --format=%H
[color]
    ui = true
    branch = auto
    diff = auto
    interactive = auto
    status = auto
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan
[merge]
	log = true
[push]
	default = matching
[rebase]
	autosquash = true
